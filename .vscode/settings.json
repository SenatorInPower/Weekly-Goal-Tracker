{
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.gitignore": true,
        "**/.gitmodules": true,
        "**/.gitattributes": true,
        "**/.vs": true,
        "**/.vsconfig": true,
        "**/.vscode": false,
        "**/Library": true,
        "**/Temp": true,
        "**/Logs": true,
        "**/UserSettings": true,
        "**/ProjectSettings": true,
        "**/Packages": true,
        "**/obj": true,
        "**/build": true,
        "**/Build": true,
        "**/Builds": true,
        "*.unityproj": true,
        "*.user": true,
        "*.userprefs": true,
        "*.suo": true,
        "*.tmp": true,
        "*.pidb": true,
        "*.booproj": true,
        "**/*.unity": true,
        "**/*.prefab": true,
        "**/*.mat": true,
        "**/*.asset": true,
        "**/*.meta": true,
        "**/*.wav": true,
        "**/*.mp3": true,
        "**/*.ogg": true,
        "**/*.pdf": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.obj": true,
        "**/*.OBJ": true,
        "**/*.3ds": true,
        "**/*.dae": true,
        "**/*.blend": true,
        "**/*.psd": true,
        "**/*.tga": true,
        "**/*.tif": true,
        "**/*.tiff": true,
        "**/*.exr": true,
        "**/*.hdr": true,
        "Assets/Animations": true,
        "Assets/Audio": true,
        "Assets/Editor": true,
        "Assets/Fonts": true,
        "Assets/Gizmos": true,
        "Assets/Materials": true,
        "Assets/Models": true,
        "Assets/Plugins": true,
        "Assets/Prefabs": true,
        "Assets/Resources": true,
        "Assets/Scenes": true,
        "Assets/Settings": true,
        "Assets/Shaders": true,
        "Assets/Sprites": true,
        "Assets/StreamingAssets": true,
        "Assets/Textures": true,
        "Assets/TextMesh Pro": true,
        "Assets/UI": true,
        "Assets/XR": true,
        "Assets/XRI": true
    },
    "search.exclude": {
        "**/Library": true,
        "**/Temp": true,
        "**/Logs": true,
        "**/UserSettings": true,
        "**/ProjectSettings": true,
        "**/Packages": true,
        "**/obj": true,
        "**/*.meta": true
    },
    "editor.mouseWheelZoom": true,
    "editor.fontSize": 12,
    "editor.lineHeight": 18,
    "terminal.integrated.fontSize": 12,
    "debug.console.fontSize": 12,
    "markdown.preview.fontSize": 12,
    "window.zoomLevel": -1,
    "editor.minimap.enabled": true,
    "editor.minimap.size": "proportional",
    "editor.minimap.maxColumn": 150,
    "editor.minimap.scale": 2,
    "editor.minimap.renderCharacters": true,
    "editor.minimap.side": "right",
    "editor.minimap.showSlider": "mouseover",
    "editor.codeLens": false,
    "editor.glyphMargin": false,
    "editor.lineNumbers": "on",
    "editor.renderWhitespace": "none",
    "editor.rulers": [],
    "editor.folding": true,
    "editor.showFoldingControls": "mouseover",
    "editor.scrollBeyondLastLine": false,
    "editor.padding.top": 0,
    "editor.padding.bottom": 0,
    "breadcrumbs.enabled": false,
    "workbench.statusBar.visible": true,
    "workbench.layoutControl.enabled": false,
    "workbench.editor.showTabs": "multiple",
    "workbench.editor.tabSizing": "shrink",
    "workbench.sideBar.location": "left",
    "window.density.editorTabHeight": "compact",
    "editor.wordWrap": "off",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.suggestSelection": "first",
    "editor.snippetSuggestions": "top",
    "editor.suggest.insertMode": "replace",
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.stickyScroll.enabled": true,
    "editor.inlineSuggest.enabled": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "omnisharp.useModernNet": false,
    "omnisharp.useGlobalMono": "never",
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.enableEditorConfigSupport": true,
    "dotnet.defaultSolution": "Arduino Raspberry Pi Project.sln",
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp",
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.detectIndentation": false
    },
    "unity.suppressCommonWarnings": true,
    "unity.suppressDotnetInstallWarning": true,
    "unity.suppressProjectGenerationWarning": true,
    "csharp.suppressDotnetInstallWarning": true,
    "csharp.suppressHiddenDiagnostics": true,
    "explorer.compactFolders": true,
    "explorer.sortOrder": "type",
    "explorer.openEditors.visible": 0,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.cs": "${capture}.cs.meta",
        "*.sln": "*.csproj, .vsconfig",
        "*.asmdef": "*.asmdef.meta"
    },
    "workbench.tree.indent": 8,
    "workbench.tree.renderIndentGuides": "none",
    "files.associations": {
        "*.cs": "csharp",
        "*.shader": "shaderlab",
        "*.cginc": "shaderlab",
        "*.hlsl": "hlsl",
        "*.compute": "hlsl",
        "*.asset": "yaml",
        "*.meta": "yaml",
        "*.prefab": "yaml",
        "*.unity": "yaml",
        "*.mat": "yaml",
        "*.anim": "yaml",
        "*.controller": "yaml",
        "*.overrideController": "yaml",
        "*.inputactions": "json"
    },
    "editor.smoothScrolling": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": "off",
    "workbench.list.smoothScrolling": false,
    "terminal.integrated.smoothScrolling": false,
    "files.autoSave": "off",
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.cursorBlinking": false,
    "terminal.integrated.enablePersistentSessions": false,
    "git.enabled": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.decorations.enabled": false,
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "material-icon-theme",
    "telemetry.telemetryLevel": "off",
    "security.workspace.trust.enabled": false,
    "update.mode": "manual",
    "dotnet.formatting.organizeImportsOnFormat": true,
    "editor.guides.indentation": false
}
